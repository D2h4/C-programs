#include<stdio.h>
#include<conio.h>
int main()
{
    int a,b,c,d,r1,r2;
    printf("Enter the values of a,b,c: ");
    scanf("%d%d%d",&a,&b,&c);
    d=(b*b)-(4*a*c);
    if(d>0)
       {
           r1=(-b+sqrt(d))/(2*a);
           r2=(-b-sqrt(d))/(2*a);
           printf("Root1: %d\tRoot2: %d",r1,r2);
       }
     else if(d==0)
     {
         r1=-b/(2*a);
         r2=-b/(2*a);
          printf("Root1: %d\tRoot2: %d",r1,r2);
     }
     else{
        printf("the roots are imaginary");
     }
   return 0;
}
