#include<stdio.h>
int main()
{   int n;
    printf("enter the number");
    scanf("%d",&n);
    if(n%5==0)
    {
        printf("it is divisible by 5");
    }
    else if(n%2==0)
    {
        printf("it is divisible by 2");
    }
    else if(n%3==0)
     { 
        printf("it is divisible by 3");
     }
     else
     {
        printf("not divisible");
     }
    return 0;
}

