//read and print the data of the file.
#include<stdio.h>
int main()
{
    FILE *fptr;
    fptr=fopen("text.txt","r");
    char ch;
    fscanf(fptr,"%c",&ch);
    printf("%c",ch);
    fscanf(fptr,"%c",&ch);
    printf("%c",ch);
    fscanf(fptr,"%c",&ch);
    printf("%c",ch);
    fscanf(fptr,"%c",&ch);
    printf("%c",ch);
    fscanf(fptr,"%c",&ch);
    printf("%c",ch);
    fclose(fptr);
    return 0;
}
