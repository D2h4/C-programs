//avg of five numbers by using functions( accept parameter, return value)
#include<stdio.h>
int avg(int a,int b,int c,int d,int e);//fn prototyping
int main()
{   int a,b,c,d,e;
   printf("enter the five numbers: ");
   scanf("%d%d%d%d%d",&a,&b,&c,&d,&e);
   int n=avg(a,b,c,d,e);//fn calling
   printf("%d",n);
    return 0;
}
//call by value
int avg(int a,int b,int c,int d,int e)//fn defination
{
     return (a+b+c+d+e)/5;
}



