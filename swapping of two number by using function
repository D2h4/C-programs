//swapping of two number by using function
#include<stdio.h>
void swap(int x,int y);//function prototyping
int main()
{   int x,y;
     printf("enter the value of x,y: ");
   scanf("%d%d",&x,&y);
    swap(x,y);//function calling
     printf("\n main fn value: %d %d",x,y);
    return 0;
}
//call by value
void swap(int m,int n)//function defination
{  int temp;
   temp=m;
   m=n;
   n=temp;
   printf("other fn value: %d %d",m,n);
}



