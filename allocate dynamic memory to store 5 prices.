//WAP to allocate memory to store 5 prices.
#include<stdio.h>
#include<stdlib.h>
int main()
{
    float *ptr;
    ptr=malloc(5*sizeof(float));
    ptr[0]=56.88;
     ptr[1]=53.23;
      ptr[2]=86.82;
       ptr[3]=26.18;
        ptr[4]=59.28;
        for( int i=0;i<5;i++)
        {
            printf("%f\n",ptr[i]);
        }
        return 0;

}
