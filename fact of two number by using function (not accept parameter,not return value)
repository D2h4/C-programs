//fact of two number by using function (not accept parameter,not return value)
#include<stdio.h>
void fact();//function prototyping
int main()
{

    fact();//function calling

    return 0;
}
//call by value
void fact()//function defination
{
   int i=1,n,fact=1;
   printf("enter the value of n: ");
   scanf("%d",&n);
   while(i<=n)
   {
       fact*=i;
       i++;
   }
   printf("%d",fact);
}
