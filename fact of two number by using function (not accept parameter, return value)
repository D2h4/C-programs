//fact of two number by using function (not accept parameter, return value)
#include<stdio.h>
int fact();//function prototyping
int main()
{   int n=fact();//function calling
     printf("%d",n);
    return 0;
}
//call by value
int fact()//function defination
{  int fact=1,i=1,n;
    printf("enter the value of n: ");
   scanf("%d",&n);
   while(i<=n)
   {
       fact*=i;
       i++;
   }
   return fact;
}

