//fact of two number by using function ( accept parameter,not return value)
#include<stdio.h>
void fact(int n);//function prototyping
int main()
{
     int n;
   printf("enter the value of n: ");
   scanf("%d",&n);

    fact(n);//function calling

    return 0;
}
//call by value
void fact(int n)//function defination
{  int fact=1,i=1;

   while(i<=n)
   {
       fact*=i;
       i++;
   }
   printf("%d",fact);
}
