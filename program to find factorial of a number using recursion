#include<stdio.h>
#include<conio.h>
int fact(int);
void main()
{
    int n,result;
    printf("enter the value of n: ");
    scanf("%d",&n);
    result=fact(n);
    printf("%d",result);
    getch();
}
int fact(int k)
{
    if(k==0)
    {
        return 1;
    }
    else{
        return k*fact(k-1);
    }
}
