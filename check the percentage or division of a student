
#include<stdio.h>
int main()
{  int m,n;
   float per;
    printf("enter the marks: ");
    scanf("%d%d",&m,&n);
    per=(n*100)/m;
    if(per>=70)
     {
        printf("ist division");

     }
     else
     {
        printf("sorry");
     }
    return 0;
}

------------------------------------------------------------------------
#include<stdio.h>
int main()
{
    int m,n;
    int per;
    printf("enter the marks: ");
    scanf("%d%d",&m,&n);
    per=(n*100)/m;
    if(per>70)
    {
        printf("ist division");
    }
    else if(per<=70 && per>55)
    {
        printf("second division");
    }
    else if(per<=55 && per>33)
    {
        printf("third division");
    }
    else
    {
        printf("fail");
    }
    return 0;
}
