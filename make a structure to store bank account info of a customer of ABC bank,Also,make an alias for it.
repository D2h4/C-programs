//make a structure to store bank account info of a customer of ABC bank,Also,make an alias for it.
#include<stdio.h>
typedef struct bankaccount{
   int accountno;
   char name[100];
} acc;
int main()
{  acc a1={345,"AMAN"};
   acc a2={235,"SURAJ"};

   printf("%d\n",a1.accountno);
   printf("%s\n",a1.name);

     printf("%d\n",a2.accountno);
   printf("%s",a2.name);

   return 0;

}
