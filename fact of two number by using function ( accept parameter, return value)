//fact of two number by using function ( accept parameter, return value)
#include<stdio.h>
int fact(int n);//function prototyping
int main()
{   int n;
     printf("enter the value of n: ");
   scanf("%d",&n);
    int b=fact(n);//function calling
     printf("%d",b);
    return 0;
}
//call by value
int fact(int n)//function defination
{  int fact=1,i=1;

   while(i<=n)
   {
       fact*=i;
       i++;
   }
   return fact;
}


