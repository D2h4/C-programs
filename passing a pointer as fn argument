//passing an pointer( in place of array bcoz array is a pointer) as function argument
#include<stdio.h>
void printarr(int *arr,int n);
int main()
{
    int arr[]={1,2,3,4,5,6};
    printarr(arr,6);
    return 0;
}

void printarr(int *arr,int n)
{
    for(int i=0;i<n;i++)
    {
        printf("%d \t",arr[i]);
    }
}
