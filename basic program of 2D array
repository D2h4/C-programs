//2D Array
#include<stdio.h>

int main()
{   int marks[3][3];
    for(int i=0;i<3;i++)
    {
        for(int j=0;j<3;j++)
        {
            scanf("%d",&marks[i][j]);
        }
    }

     for(int i=0;i<3;i++)
    {
        for(int j=0;j<3;j++)
        {
            printf("%d \t",marks[i][j]);
        }
    }

    return 0;
}



