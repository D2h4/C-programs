//WAP that inputs user name(string) and prints its length.
#include<stdio.h>
int countlength(char arr[])
{   int count=0;
     for(int i=0;arr[i]!='\0';i++)
     {
         count++;
     }
     return count-1;

}
int main()
{ char name[100];
  fgets(name,100,stdin);
  printf("%d",countlength(name));
  return 0;

}
